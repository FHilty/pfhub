---
data:
{%- for name, datum in data %}
  - name: {{ name }}
    code:
      name: {{ datum.metadata.software.name }}
      url: {{ datum.metadata.implementation.repo.url }}
    id_: {{ datum.benchmark_id }}
    author:
      email: {{ datum.metadata.email }}
      name: {{ datum.metadata.author }}
    timestamp: {{ datum.metadata.timestamp }}
    cores: {{ datum.metadata.hardware.cores }}
{%- endfor %}
